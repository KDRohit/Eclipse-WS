<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans.xsd"
           default-init-method="defaultInit"
           default-destroy-method="defaultDestroy"
           >

    <!-- Address Bean -->
    
    <bean id="ldt" class="java.time.LocalDateTime" factory-method="now"/>
    <bean id="smg" class="com.ig.sbeans.SeasonMessageGenerator" destroy-method="destroy" autowire="constructor">
    	<property name="name" value="rohit_override_by_setter_injection"/>
    	<constructor-arg name="ldt" ref="ldt"/>
    	<constructor-arg name="name" value="rohit"/>
    	<constructor-arg/>
    	
    </bean>
    <bean id="b" factory-bean="factory" factory-method="getInstanceB" primary="true"/>
    <bean id="factory" class="com.ig.sbeans.UserDefineClassFactory"/>
    <bean id="a" class = "com.ig.sbeans.UserDefineClassFactory" factory-method="getInstanceA"/>
    
</beans>
